{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "parser": "@babel/eslint-parser",
  "rules": {
    "semi": ["error", "never"],
    "max-len": ["error", { "ignoreComments": true }],
    "no-console": "off"
  }
}
